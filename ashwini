
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication9
{
    class Program
    {
        static void Main(string[] args)
        {
            string fruit = "Apple";
            switch (fruit)
            {
                case "Banana":
                    Console.Write(fruit + "is the delecious fruit");
                    break;
                case "chair":
                    Console.Write(fruit + "is the delecious fruit");
                    break;
                case "Apple":
                    goto case "Banana";
                    break;
            }
            Console.ReadKey();
        }
    }
}







